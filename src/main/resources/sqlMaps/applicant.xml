<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.digigroove.smartflat.dao.ApplicantDAO">

    <!-- 구매문의 -->
    <insert id="setApplicant" parameterType="applicantEntity">
        INSERT INTO
        applicant
        (
            app_type
            ,category
            ,name
            ,tel
            ,email
            ,subject
            ,company
            ,content
            ,reg_date
        )
        VALUES
        (
            'S'
            ,'스마트플렛'
            ,#{name}
            ,#{tel}
            ,#{email}
            ,#{subject}
            ,''
            ,#{content}
            ,NOW()
        )
    </insert>
</mapper>